	keywords = {Access to Finance, Business in Development, Competitiveness and Competition Policy, E-Business, Economic Theory \& Research},
	file = {Snapshot:/Users/Justin/Library/Application Support/Zotero/Profiles/0rdsolen.default/zotero/storage/T9JSSGQE/papers.html:text/html}
