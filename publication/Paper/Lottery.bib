	keywords = {Individual decision making, Lotteries, Online experiment, Personal finance, Savings},
	pages = {86--106},
	file = {ScienceDirect Full Text PDF:/Users/Justin/Library/Application Support/Zotero/Profiles/0rdsolen.default/zotero/storage/C8C3NMS4/Atalay et al. - 2014 - Savings and prize-linked savings accounts.pdf:application/pdf;ScienceDirect Snapshot:/Users/Justin/Library/Application Support/Zotero/Profiles/0rdsolen.default/zotero/storage/2EVRMWIP/S0167268114002194.html:text/html}
