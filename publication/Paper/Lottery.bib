@techreport{cookson_when_2016,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {When {Saving} is {Gambling}},
	url = {http://papers.ssrn.com/abstract=2517126},
	abstract = {This paper studies how individuals change gambling behavior when offered Prize-Linked Savings (PLS) accounts, which provide lottery-like interest payoffs instead of a fixed rate of interest. My analysis combines transaction-level data on cash withdrawals at casinos with a natural experiment in the introduction of lottery-linked savings accounts in Nebraska at the beginning of 2012. After the introduction of lottery-linked savings, consumers in affected regions reduce casino gambling by 7 to 15 percent relative to unaffected regions. This effect is stronger when savings lotteries and casino gambling are similar, consistent with the interpretation that the effect is due to attribute-based substitution. In addition, I find that PLS leads to stronger substitution when gambling patrons exhibit relatively greater sophistication in how they access cash. Although PLS accounts effectively redirect gambling expenditure into savings, these findings suggest that the introduction of PLS does not merely substitute for better financial sophistication. Indeed, the impact of PLS on casino gambling is greater for consumers with better ex ante financial habits.},
	number = {ID 2517126},
	urldate = {2016-04-22},
	institution = {Social Science Research Network},
	author = {Cookson, J. Anthony},
	month = jan,
	year = {2016},
	keywords = {casino gambling, gambling markets, household finance, lottery, Prize Linked Savings},
	file = {Snapshot:/Users/Justin/Library/Application Support/Zotero/Profiles/0rdsolen.default/zotero/storage/BQJV75WW/papers.html:text/html}
}

